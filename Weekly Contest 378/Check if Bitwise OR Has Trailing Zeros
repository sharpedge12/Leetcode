class Solution {
    public boolean hasTrailingZeros(int[] nums) {
        int ev = 0 ;
        for(int i =0 ; i< nums.length ; i++){
            if(nums[i]%2 == 0){
                ev++;
            }
        }
        
        if(ev > 1){
            return true;
        }else{
            return false;
        }
        
    }
}
